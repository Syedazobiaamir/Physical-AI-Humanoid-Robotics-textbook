# Backend API URL
REACT_APP_API_BASE_URL=http://localhost:8000/api/v1

# Authentication (BetterAuth)
REACT_APP_AUTH_PROVIDER=google
REACT_APP_CLIENT_ID=your_oauth_client_id_here
